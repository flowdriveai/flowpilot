 <customMathEquations>
  <snippet name="dv/dt">
   <global>prevX = 0
prevY = 0
is_first = true</global>
   <function>if (is_first) then
  is_first = false
  prevX = time
  prevY = value
end

dx = time - prevX
dy = value - prevY
prevX = time
prevY = value

return dy/dx</function>
   <linkedSource>/carState/vEgo</linkedSource>
  </snippet>
 </customMathEquations>
